{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    [
      "babel-plugin-transform-inline-environment-variables",
      {
        "include": [
          "npm_package_version"
        ]
      }
    ]
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [
            "last 2 versions"
          ]
        },
        "modules": "commonjs"
      }
    ],
    "@babel/react"
  ]
}
