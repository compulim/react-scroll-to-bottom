{
  "env": {
    "test": {
      "plugins": [
        "babel-plugin-istanbul"
      ]
    }
  },
  "plugins": [
    [
      "babel-plugin-transform-inline-environment-variables",
      {
        "include": [
          "npm_package_version"
        ]
      }
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ],
  "presets": [
    "@babel/react"
  ]
}
